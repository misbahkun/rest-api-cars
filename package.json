  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "cloudinary": "^1.41.0",
    "dotenv": "^16.3.1",
